<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#011529"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/btn_B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="B"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintRight_toLeftOf="@id/btn_C"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/btn_C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        app:layout_constraintRight_toLeftOf="@id/btn_D"

        app:layout_constraintBaseline_toBaselineOf="@id/btn_B"
        app:layout_constraintLeft_toRightOf="@id/btn_B"
        />
    <Button
        android:id="@+id/btn_D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="D"

        app:layout_constraintBaseline_toBaselineOf="@id/btn_B"
        app:layout_constraintLeft_toRightOf="@id/btn_C"
        app:layout_constraintRight_toRightOf="parent"


        />
    <Button
        android:id="@+id/btn_E"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        app:layout_constraintHorizontal_bias="0.6"
        android:text="heifdfgh.kjghjgh"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintRight_toLeftOf="@id/btn_F"
        app:layout_constraintTop_toBottomOf="@+id/btn_B"
        app:layout_constraintLeft_toLeftOf="parent"

        />
    <Button
        android:id="@+id/btn_F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="F"
        app:layout_constraintRight_toLeftOf="@id/btn_G"
        app:layout_constraintBaseline_toBaselineOf="@id/btn_E"
        app:layout_constraintLeft_toRightOf="@id/btn_E"
        />
    <Button
        android:id="@+id/btn_G"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="G"
        app:layout_constraintBaseline_toBaselineOf="@id/btn_E"
        app:layout_constraintLeft_toRightOf="@id/btn_F"
        app:layout_constraintRight_toRightOf="parent"

        />
    <Button
        android:id="@+id/btn_H"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="H"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.6"
        app:layout_constraintHorizontal_weight="4"

        app:layout_constraintRight_toLeftOf="@id/btn_I"
        app:layout_constraintTop_toBottomOf="@id/btn_E"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/btn_I"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="I"
        app:layout_constraintRight_toLeftOf="@id/btn_J"
app:layout_constraintHorizontal_weight="5"
        app:layout_constraintBaseline_toBaselineOf="@id/btn_H"
        app:layout_constraintLeft_toRightOf="@id/btn_H"
        />
    <Button
        android:id="@+id/btn_J"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="j"
        app:layout_constraintHorizontal_weight="6"
        app:layout_constraintBaseline_toBaselineOf="@id/btn_H"
        app:layout_constraintLeft_toRightOf="@id/btn_I"
        app:layout_constraintRight_toRightOf="parent"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
android:id="@+id/conner"
        android:text="conor"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/corner"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_social_media"
        android:scaleType="centerInside"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintBottom_toBottomOf="parent"/>

    <androidx.cardview.widget.CardView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:cardCornerRadius="50dp"
        app:layout_constraintTop_toBottomOf="@id/btn_I"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>