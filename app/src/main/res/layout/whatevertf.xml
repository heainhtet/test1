<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/txt_H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Heain Htet Zaw"
        android:textSize="25sp"
        android:textColor="#0E0D00"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/mainw"
        android:layout_width="172dp"
        android:layout_height="206dp"
        android:src="@drawable/wiz"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/txt_la"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Los Angles,CA"
        android:textColor="#0E0D00"
        app:layout_constraintTop_toBottomOf="@id/txt_H"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="50dp"/>
    <Button
        android:id="@+id/btn_Fol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Follow"
        android:textColor="#0E0D00"
        app:layout_constraintTop_toBottomOf="@id/txt_la"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"/>
    <TextView
        android:id="@+id/txt_344"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="34"
        android:textSize="20sp"
        android:textColor="#020000"
        app:layout_constraintTop_toBottomOf="@id/btn_Fol"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1256"
        android:id="@+id/text_125"
        app:layout_constraintTop_toBottomOf="@id/btn_Fol"
        android:textSize="20sp"
        android:textColor="#020000"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="120dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Post"
        app:layout_constraintTop_toBottomOf="@id/txt_344"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="30dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Followers"
        app:layout_constraintTop_toBottomOf="@id/text_125"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="120dp"/>
    <ImageView
        android:id="@+id/img1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/wiz"
        app:layout_constraintTop_toBottomOf="@id/txt_344"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/img2"
        app:layout_constraintHorizontal_chainStyle="packed"
        android:layout_marginTop="30dp"/>
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:id="@+id/img2"
            android:src="@drawable/wiz"
            app:layout_constraintTop_toBottomOf="@id/text_125"
            app:layout_constraintLeft_toRightOf="@id/img1"
            android:layout_marginTop="30dp"
            app:layout_constraintRight_toLeftOf="@id/img3"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/wiz"
        android:id="@+id/img3"
        app:layout_constraintTop_toBottomOf="@id/text_125"
        app:layout_constraintLeft_toRightOf="@id/img2"
        app:layout_constraintRight_toLeftOf="@id/img4"
        android:layout_marginTop="30dp"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/wiz"
        android:id="@+id/img4"
        app:layout_constraintTop_toBottomOf="@id/text_125"
        app:layout_constraintLeft_toRightOf="@id/img3"
        app:layout_constraintRight_toLeftOf="@id/img5"
        android:layout_marginTop="30dp"/>
    <ImageView
        android:id="@+id/img5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:src="@drawable/wiz"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/text_125"
        app:layout_constraintLeft_toRightOf="@id/img4"
        app:layout_constraintRight_toRightOf="parent"/>
    <ImageView
        android:id="@+id/ii"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_social_media"
        app:layout_constraintTop_toBottomOf="@id/img1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ig"
        app:layout_constraintHorizontal_chainStyle="spread"

        android:layout_marginTop="15dp"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/isg"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintLeft_toRightOf="@id/ii"
        app:layout_constraintRight_toLeftOf="@id/twt"

        android:layout_marginTop="15dp"
        android:id="@+id/ig"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/twitter"
        app:layout_constraintTop_toBottomOf="@id/img3"
        app:layout_constraintLeft_toRightOf="@id/ig"
        app:layout_constraintRight_toLeftOf="@id/beh"
        android:layout_marginTop="15dp"

        android:id="@+id/twt"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_behance"
        app:layout_constraintTop_toBottomOf="@id/img4"
        app:layout_constraintLeft_toRightOf="@id/twt"
        app:layout_constraintRight_toLeftOf="@id/isss"
        android:layout_marginTop="15dp"

        android:id="@+id/beh"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/img5"
        app:layout_constraintLeft_toRightOf="@id/beh"
        android:src="@drawable/isst"
        android:layout_marginTop="15dp"

        android:id="@+id/isss"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/vie"
        android:layout_width="410dp"
        android:layout_height="1.5dp"
        android:background="#0C0202"
        app:layout_constraintTop_toBottomOf="@id/twt"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/abtid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About"
        app:layout_constraintTop_toBottomOf="@id/vie"
        android:textSize="30sp"
        android:textColor="#070000"
        android:layout_marginTop="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="10dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Him poetic eye. Searched any remedies. Trial. Was
poverty schemes him he dressing mountains and as surprise haven't
subject and could a entered would in the of be not posts, doing this never publication"
        app:layout_constraintTop_toBottomOf="@id/abtid"
        android:textSize="20sp"
        android:layout_marginTop="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="10dp"
        android:textColor="#070000"/>


</androidx.constraintlayout.widget.ConstraintLayout>